  <div class="display-6 mb-4">
    <h1>My Places</h1>
  </div>

<% if @current_user %>
  <div>
    <a class="btn btn-primary mb-4" href="/places/new">New place</a>
  </div>

  <ul class="list-group list-group-flush">
    <% if @current_user %>
      <% for place in @places %>
        <li class="list-group-item"><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></li>
      <% end %>
    <% end %>
<% end %>
  </ul>